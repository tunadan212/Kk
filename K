end)

-- ðŸ§  Duplicate v2 â€” cháº¡y loadstring + hiá»ƒn thá»‹ tiáº¿n Ä‘á»™ 10s vÃ  Success, KHÃ”NG toast
local btnClone2=pill(body,"ðŸ§  Duplicate v2"); pillColor(btnClone2,114,106,240)
btnClone2.MouseButton1Click:Connect(function()
    btnClone2.Text="Duplicating..."; pillColor(btnClone2,70,200,90)
    ShowProgress10s()
    task.defer(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/tunadan212/Kkkk/refs/heads/main/K"))()
    end)
end)

